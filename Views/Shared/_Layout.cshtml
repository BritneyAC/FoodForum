<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Lazy Vegan Recipes</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <script type="text/javascript">
            var RootUrl = '@Url.Content("~/")';
            var PartialUrls = PartialUrls || {};
            PartialUrls.Username = '@Url.Action("UsernameAsync", "User")';
            PartialUrls.LoginUsername = '@Url.Action("LoginUsernameAsync", "User")';
            PartialUrls.RecipeTitle = '@Url.Action("RecipeTitleAsync", "Admin")';
            PartialUrls.UserRecipeTitle = '@Url.Action("UserRecipeTitleAsync", "UserRecipes")';
            PartialUrls.IndexRating = '@Url.Action("IndexRatingAsync", "Home")';
            PartialUrls.IndexRatingReverse = '@Url.Action("IndexRatingReverseAsync", "Home")';
            PartialUrls.UserRating = '@Url.Action("UserRatingAsync", "Home")';
            PartialUrls.UserRatingReverse = '@Url.Action("UserRatingReverseAsync", "Home")';
            PartialUrls.IndexLikes = '@Url.Action("IndexLikesAsync", "Home")';
            PartialUrls.IndexLikesReverse = '@Url.Action("IndexLikesReverseAsync", "Home")';
            PartialUrls.UserLikes = '@Url.Action("UserLikesAsync", "Home")';
            PartialUrls.UserLikesReverse = '@Url.Action("UserLikesReverseAsync", "Home")';
            PartialUrls.IndexTime = '@Url.Action("IndexTimeAsync", "Home")';
            PartialUrls.IndexTimeReverse = '@Url.Action("IndexTimeReverseAsync", "Home")';
            PartialUrls.UserTime = '@Url.Action("UserTimeAsync", "Home")';
            PartialUrls.UserTimeReverse = '@Url.Action("UserTimeReverseAsync", "Home")';
        </script>
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.css" />
        <script type="text/javascript">
            var RootUrl = '@Url.Content("~/")';
            var PartialUrls = PartialUrls || {};
            PartialUrls.Username = '@Url.Action("UsernameAsync", "User")';
            PartialUrls.LoginUsername = '@Url.Action("LoginUsernameAsync", "User")';
            PartialUrls.RecipeTitle = '@Url.Action("RecipeTitleAsync", "Admin")';
            PartialUrls.UserRecipeTitle = '@Url.Action("UserRecipeTitleAsync", "UserRecipes")';
            PartialUrls.IndexRating = '@Url.Action("IndexRatingAsync", "Home")';
            PartialUrls.IndexRatingReverse = '@Url.Action("IndexRatingReverseAsync", "Home")';
            PartialUrls.UserRating = '@Url.Action("UserRatingAsync", "Home")';
            PartialUrls.UserRatingReverse = '@Url.Action("UserRatingReverseAsync", "Home")';
            PartialUrls.IndexLikes = '@Url.Action("IndexLikesAsync", "Home")';
            PartialUrls.IndexLikesReverse = '@Url.Action("IndexLikesReverseAsync", "Home")';
            PartialUrls.UserLikes = '@Url.Action("UserLikesAsync", "Home")';
            PartialUrls.UserLikesReverse = '@Url.Action("UserLikesReverseAsync", "Home")';
            PartialUrls.IndexTime = '@Url.Action("IndexTimeAsync", "Home")';
            PartialUrls.IndexTimeReverse = '@Url.Action("IndexTimeReverseAsync", "Home")';
            PartialUrls.UserTime = '@Url.Action("UserTimeAsync", "Home")';
            PartialUrls.UserTimeReverse = '@Url.Action("UserTimeReverseAsync", "Home")';
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
</head>
<body>
    <div class="container body-content">
        @RenderBody()
    </div>

    

</body>
</html>
